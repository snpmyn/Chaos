<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/stateOn"
        android:drawable="@drawable/bottom_navigation_view_menu_mine_select_22dp"
        android:state_checked="true" />
    <item
        android:id="@+id/stateOff"
        android:drawable="@drawable/bottom_navigation_view_menu_mine_normal_22dp" />
    <transition
        android:drawable="@drawable/bottom_navigation_view_menu_mine_normal_22dp"
        android:fromId="@id/stateOff"
        android:toId="@id/stateOn" />
</animated-selector>
